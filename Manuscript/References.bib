@article{Veech2006,
abstract = {Aim  To test for non-random co-occurrence in 36 species of grassland birds using a new metric and the C-score. The analysis used presence–absence data of birds distributed among 305 sites (or landscapes) over a period of 35 years. This analysis departs from traditional analyses of species co-occurrence in its use of temporal data and of individual species' probabilities of occurrence to derive analytically the expected co-occurrence between paired species. Location  Great Plains region, USA. Methods  Presence–absence data for the bird species were obtained from the North American Breeding Bird Survey. The analysis was restricted to species pairs whose geographic ranges overlapped. Each of 541 species pairs was classified as a positive, negative, or non-significant association depending on the mean difference between the observed and expected frequencies of co-occurrence over the 35-year time-span. Results  Of the 541 species pairs that were examined, 202 to 293 (37–54{\%}) were positively associated, depending on which of two null models was used. However, only a few species pairs ({\textless}5{\%}) were negatively associated. An additional 89 species pairs did not have overlapping ranges and hence represented de facto negative associations. The results from analyses based on C-scores generally agreed with the analyses based on the difference between observed and expected co-occurrence, although the latter analyses were slightly more powerful. Main conclusions  Grassland birds within the Great Plains region are primarily distributed among landscapes either independently or in conjunction with one another. Only a few species pairs exhibited repulsed or segregated distributions. This indicates that the shared preference for grassland habitat may be more important in producing coexistence than are negative species interactions in preventing it. The large number of non-significant associations may represent random associations and thereby indicate that the presence/absence of other grassland bird species may have little effect on whether a given focal species is also found within the landscape. In a broader context, the probability-based approach used in this study may be useful in future studies of species co-occurrence.},
annote = {Veech employs his probabilistic approach for the first time using BBS data in the great plains. He deliberately removes species pairs that do not overlap geographically, effectively removing on biogeographic segregations. His results reflect many more significant aggregations than segregations. However, a smaller analysis with theoretically competing sparrow species also turns up more aggregations showing that competition doesn't increase segregations even on a pretty good spatial resolution. He reports comparable results from a more mainstream co-occurrence approach and suggests the probabilistic method may be useful for other studies. 

This is in contrast to ETE analyses where biogeographic agg/seg make up a good chunk of significant associations. This may be why he has so many more aggs but not necessarily in light of the sparrow analysis. He uses temporal data but only averages over time instead of looking at change over time. May be useful to acquire this dataset and use it as a temporal network thing. He fails to check if clusters are formed despite the very dynamic nature of most individual links. This would be something I'd want to investigate with this data.},
author = {Veech, Joseph A.},
doi = {10.1111/j.1365-2699.2006.01571.x},
file = {:Users/aniko/Library/Application Support/Mendeley Desktop/Downloaded/Veech - 2006 - A probability-based analysis of temporal and spatial co-occurrence in grassland birds.pdf:pdf},
isbn = {0305-0270},
issn = {03050270},
journal = {Journal of Biogeography},
keywords = {Breeding Bird Survey,Dispersal-assembly,Great Plains,Habitat selection,Landscape,North America,Presence-absence matrix,Species coexistence,Species diversity,Species interactions},
number = {12},
pages = {2145--2153},
title = {{A probability-based analysis of temporal and spatial co-occurrence in grassland birds}},
volume = {33},
year = {2006}
}
